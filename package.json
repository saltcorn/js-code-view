{
  "name": "@saltcorn/code-view",
  "version": "0.1.11",
  "description": "Generate Saltcorn view output from code",
  "main": "index.js",
  "dependencies": {
    "@saltcorn/markup": "^0.8.1",
    "@saltcorn/data": "^0.8.1",
    "handlebars": "4.7.7"
  },
  "author": "Tom Nielsen",
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "repository": "github:saltcorn/js-code-view",
  "scripts": {
    "test": "jest tests --runInBand"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "no-unused-vars": "off",
      "no-case-declarations": "off",
      "no-empty": "warn",
      "no-fallthrough": "warn"
    }
  }
}
